<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="NVARCHAR" hibernate-type='String'
			not-null="false">
		</sql-type>
		<sql-type jdbc-type="TINYINT" hibernate-type="java.lang.Integer"
			not-null="false">
		</sql-type>
	</type-mapping>
	<table-filter match-name="projects" match-catalog="dailyreport"
		match-schema="dbo">
	</table-filter>
	<table-filter match-name="reports" match-catalog="dailyreport"
		match-schema="dbo">
	</table-filter>
	<table-filter match-name="rights" match-catalog="dailyreport"
		match-schema="dbo">
	</table-filter>
	<table-filter match-name="tasks" match-catalog="dailyreport"
		match-schema="dbo">
	</table-filter>
	<table-filter match-name="users" match-catalog="dailyreport"
		match-schema="dbo">
	</table-filter>
	<table catalog="dailyreport" schema="dbo" name="projects"
		class="com.dailyreport.model.Project">
		<column name="id"></column>
		<column name="project_code"></column>
		<column name="finish_at"></column>
		<column name="name"></column>
		<column name="created_at"></column>
		<column name="updated_at"></column>
		<column name="start_at"></column>
	</table>
	<table catalog="dailyreport" schema="dbo" name="reports"
		class="com.dailyreport.model.Report">
		<column name="id"></column>
		<column name="worked_at"></column>
		<column name="approval_status"></column>
		<column name="approved_at"></column>
		<column name="user_id"></column>
		<column name="updated_at"></column>
		<column name="project_id"></column>
		<column name="task_id"></column>
		<column name="note"></column>
		<column name="approver"></column>
		<column name="created_at"></column>
		<column name="worked_to"></column>
	</table>
	<table catalog="dailyreport" schema="dbo" name="rights"
		class="com.dailyreport.model.Rights">
		<column name="id"></column>
		<column name="name"></column>
		<column name="lv"></column>
		<column name="created_at"></column>
		<column name="updated_at"></column>
	</table>
	<table catalog="dailyreport" schema="dbo" name="tasks" class="com.dailyreport.model.Task">
		<column name="id"></column>
		<column name="created_at"></column>
		<column name="name"></column>
		<column name="updated_at"></column>
		<column name="task_code"></column>
	</table>
	<table catalog="dailyreport" schema="dbo" name="users" class="com.dailyreport.model.User">
		<column name="id"></column>
		<column name="employee_code"></column>
		<column name="created_at"></column>
		<column name="name"></column>
		<column name="rights"></column>
		<column name="password"></column>
		<column name="updated_at"></column>
	</table>
</hibernate-reverse-engineering>